<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/eventDetailsFragment">
    <fragment
        android:id="@+id/eventDetailsFragment"
        android:name="com.example.navigation_graph_unit3.EventDetailsFragment"
        android:label="fragment_event_details"
        tools:layout="@layout/fragment_event_details" >
        <action
            android:id="@+id/action_eventDetailsFragment_to_timeAnddate"
            app:destination="@id/timeAnddate" />
    </fragment>
    <fragment
        android:id="@+id/timeAnddate"
        android:name="com.example.navigation_graph_unit3.TimeAnddate"
        android:label="TimeAnddate" >
        <action
            android:id="@+id/action_timeAnddate_to_blankFragment"
            app:destination="@id/blankFragment" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.navigation_graph_unit3.BlankFragment"
        android:label="BlankFragment" />
</navigation>